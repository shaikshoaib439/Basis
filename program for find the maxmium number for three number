#include<stdio.h>
int main(){
  double s1,s2,s3;
  printf("enter the three numbers");
  scanf("%lf%lf%lf",&s1,&s2,&s3);
  if(s1>=s2 && s1>=s3){
printf("the maximum number is %lf\n",s1);
  }
  if(s2>=s3 && s2>=s1){
 printf("the maximum number is %lf\n",s2);
  }
  else{
printf("the maximum number is %lf\n",s3);
  }
  if(s1==s2 || s2==s3){
    printf("you entered number is same %lf",s1);
    }
  
   return 0;
}
